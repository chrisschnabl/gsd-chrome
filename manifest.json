{
  "name": "Lovable A/B Test Assistant",
  "description": "Provides you with alterations on your Lovable prompts, to allow you to A/B test them.",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ]
}
